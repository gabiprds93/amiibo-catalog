.SearchBar
  align-items: center;
  display: flex;
  flex: 1;
  border: 1px solid $color-border;
  border-radius: 4.2rem;
  justify-content: space-between;
  margin-left: 1.6rem;
  padding: 0 2.4rem;

  & input
    border: none;
    border-radius: 4.2rem;
    outline: none;
    padding: 1.6rem 1.6rem 1.6rem 0;

  &__icon
    width: 2.4rem;